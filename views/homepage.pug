doctype html
html 
  head 
    title= `${title}`
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="/css/style.css")
  body


    h1 Custom Object "Snacks" Table

    a(href="/update-cobj") Add to this table

    table
      thead
        tr
          th Name
          th Heerlijkheidsscore
          th Pittigheidsgraad
      tbody
        each snack in data
          tr
            td= snack.properties.name || ''
            td= snack.properties.heerlijkheidsscore || ''
            td= snack.properties.pittigheidsgraad || ''